{
  "name": "@ts-c-compiler/compiler",
  "version": "1.2.0",
  "type": "module",
  "private": false,
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "dependencies": {
    "@ts-c-compiler/core": "*",
    "@ts-c-compiler/lexer": "*",
    "@ts-c-compiler/rpn": "*",
    "@ts-c-compiler/x86-assembler": "*"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "dev": "npx rollup --watch --config rollup.config.mjs",
    "build": "npx rollup --config rollup.config.mjs",
    "lint": "npx eslint --max-warnings=0 src/",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest --passWithNoTests --c ./jest.config.mjs",
    "check:types": "npx tsc --noEmit"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "publishConfig": {
    "access": "public"
  }
}
